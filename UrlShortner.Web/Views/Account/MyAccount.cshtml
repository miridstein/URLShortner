@model IEnumerable<URLShortner.Data.UrlLink>

<div class="row">
    <div class="col-md-8 col-md-offset-2">
        <table class="table table-hover table-striped table-bordered">
            <tr>
                <th>Url</th>
                <th>Views</th>
            </tr>
            @foreach (var url in Model)
            {
                <tr>
                    <td>
                        <a href="@url.Url">@url.Url</a>
                    </td>
                    <td>
                        @url.Views
                    </td>
                </tr>
            }
        </table>
    </div>
</div>